	<!-- info Shipment 
    TODO: Werte setzen
    -->
	<EventObjectGenerator name="EO_ShipmentInfo">
		<Template>
			<ShipmentInfo>
				<From><ScopeVariable name="From"/></From>
				<To><ScopeVariable name="To"/></To>
				<DeliveryDate><ValueGenerator name="VG_DateTimeHour"/></DeliveryDate>
				<TransportType>Truck</TransportType>
				<Price><ValueGenerator name="VG_Price" /></Price>
				<MinTemp><ScopeVariable name="MinTemp"/></MinTemp>
				<MaxTemp><ScopeVariable name="MaxTemp"/></MaxTemp>
			</ShipmentInfo>
		</Template>
	</EventObjectGenerator>

