	<!--
        Generator for the OrderReceived Events
    -->

	<EventGenerator name="EG_OrderReceived">
		<Template>
			<OrderReceived>
				<OrderId>
					<ValueGenerator name="VG_OrderId"/>
				</OrderId>
				<DateTime><ValueGenerator name="VG_DateTimeHour"/></DateTime>

				<!-- bug sollte ein spaeteres Datum als TransportEnd sein  -->
				<DeliveryDate>
					<ValueGenerator name="VG_DateTimeHour"/>
				</DeliveryDate>

				<Destination><ScopeVariable name="To"/></Destination>

				<ProductCollection>
					<EventObjectGenerator name="EO_ProductCollection"/>
				</ProductCollection>

			</OrderReceived>
		</Template>
	</EventGenerator>

