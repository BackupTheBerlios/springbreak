    <EventSequence name="ES_Transport_8h">
    	<EventGenerator name="EG_TransportStart_From"/>
			
			<Scope>
			<EventGenerator name="EG_TransportEnd_Ueber"/>
			<EventGenerator name="EG_DataLoggerRead8"/>
			<EventGenerator name="EG_ShipmentAudit"/>
		
			<EventGenerator name="EG_TransportStart_Ueber"/>
			<DateSequence>
					 <CorrelationData eventType="TransportStart"> 
                        <XPathSelector> 
                                <XPathExpression>//DateTime</XPathExpression> 
                        </XPathSelector> 
                </CorrelationData> 
                <CorrelationData eventType="TransportEnd"> 
                        <XPathSelector> 
                                <XPathExpression>//DateTime</XPathExpression> 
                        </XPathSelector> 
                </CorrelationData> 
			 	<RandomTimeSpans> 
                       <Min>
					   		<Minutes>30</Minutes>
					   </Min>
					   <Max>
					   	  <Hours>12</Hours>
					   </Max>
					   <Granularity>Seconds</Granularity>
                </RandomTimeSpans> 
			</DateSequence>
			
			</Scope>
			
			<EventGenerator name="EG_TransportEnd_To"/>
			<EventGenerator name="EG_DataLoggerRead8"/>
			<EventGenerator name="EG_ShipmentAudit"/>

		
