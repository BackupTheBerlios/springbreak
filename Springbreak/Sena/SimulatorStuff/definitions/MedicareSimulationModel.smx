	<EventSequence name="ES_Transport_18h">
    	<EventGenerator name="EG_TransportStart_From"/>
			
			<Scope>
			<EventGenerator name="EG_TransportEnd_Ueber"/>
			<EventGenerator name="EG_DataLoggerRead18"/>
			<EventGenerator name="EG_ShipmentAudit"/>
		
			<EventGenerator name="EG_TransportStart_Ueber"/>
			<DateSequence>
					 <CorrelationData eventType="TransportStart"> 
                        <XPathSelector> 
                                <XPathExpression>//DateTime</XPathExpression> 
                        </XPathSelector> 
                </CorrelationData> 
                <CorrelationData eventType="TransportEnd"> 
                        <XPathSelector> 
                                <XPathExpression>//DateTime</XPathExpression> 
                        </XPathSelector> 
                </CorrelationData> 
			 	<RandomTimeSpans> 
                       <Min>
					   		<Minutes>30</Minutes>
					   </Min>
					   <Max>
					   	  <Hours>12</Hours>
					   </Max>
					   <Granularity>Seconds</Granularity>
                </RandomTimeSpans> 
			</DateSequence>
			
			</Scope>
			
			<EventGenerator name="EG_TransportEnd_To"/>
			<EventGenerator name="EG_DataLoggerRead18"/>
			<EventGenerator name="EG_ShipmentAudit"/>

		
			<!-- zwischen TransportStart und TransportEnd etwa 18h -->
			<DateSequence> 
                <CorrelationData eventType="TransportStart"> 
                        <XPathSelector> 
                                <XPathExpression>//DateTime</XPathExpression> 
                        </XPathSelector> 
                </CorrelationData> 
                <CorrelationData eventType="TransportEnd"> 
                        <XPathSelector> 
                                <XPathExpression>//DateTime</XPathExpression> 
                        </XPathSelector> 
                </CorrelationData> 
			 	<RandomTimeSpans> 
                       <Min>
					   		<Minutes>1050</Minutes>
					   </Min>
					   <Max>
					   	  <Minutes>1110</Minutes>
					   </Max>
					   <Granularity>Seconds</Granularity>
                </RandomTimeSpans> 
            </DateSequence>
    </EventSequence>
    
    
	
    
    
    

	<EventSequence name="ES_Transport">
		
	   <Switch>
	   	<Case probability="0.25">
	   		<EventSequence name="ES_Transport_8h"/>
	    </Case>
		<Case probability="0.25">
	   		<EventSequence name="ES_Transport_10h"/>
	    </Case>
		<Case probability="0.25">
	   		<EventSequence name="ES_Transport_14h"/>
	    </Case>
		<Case probability="0.25">
	   		<EventSequence name="ES_Transport_18h"/>
	    </Case>
	   </Switch>

	    <!-- Nach TransportEnd kommt in 2-36min DataloggerRead, ShipmentAudit -->
            <DateSequence> 
                <CorrelationData eventType="TransportEnd"> 
                        <XPathSelector> 
                                <XPathExpression>//DateTime</XPathExpression> 
                        </XPathSelector> 
                </CorrelationData> 
                <CorrelationData eventType="DataLoggerRead"> 
                        <XPathSelector> 
                                <XPathExpression>//DateTime</XPathExpression> 
                        </XPathSelector> 
                </CorrelationData>
                 <CorrelationData eventType="ShipmentAudit"> 
                        <XPathSelector> 
                                <XPathExpression>//DateTime</XPathExpression> 
                        </XPathSelector> 
                </CorrelationData> 
                
                <RandomTimeSpans> 
                        <Min> 
                                <Minutes>2</Minutes>
                        </Min> 
                        <Max> 
                                <Minutes>25</Minutes>
                        </Max> 
                        <Granularity>Seconds</Granularity> 
                </RandomTimeSpans> 

            </DateSequence>
		
		
	</EventSequence>
		
	
	
	
	
	
	
	
	<EventSequence name="ES_Route">
	
		<ScopeVariable name="From">
					<ValueGenerator name="VG_From"/>
		</ScopeVariable>
		
		<ScopeVariable name="To">
					<ValueGenerator name="VG_To"/>
		</ScopeVariable>
		
		<ScopeVariable name="Ueber">
					<ValueGenerator name="VG_Ueber"/>
		</ScopeVariable>
		
		<ScopeVariable name="Product">
					<ValueGenerator name="VG_Product"/>
		</ScopeVariable>
		<ScopeVariable name="Amount">
					<ValueGenerator name="VG_Amount"/>
		</ScopeVariable>
		<ScopeVariable name="MinTemp">
					<ValueGenerator name="VG_MinTemp"/>
		</ScopeVariable>
		<ScopeVariable name="MaxTemp">
					<ValueGenerator name="VG_MaxTemp"/>
		</ScopeVariable>	
		
			
		<EventGenerator name="EG_OrderReceived"/>

		<EventGenerator name="EG_OrderConfirmed"/>
		
		<EventGenerator name="EG_ShipmentTenderIssued"/>
		<EventGenerator name="EG_ShipmentTenderAccepted"/>


		<EventGenerator name="EG_DataLoggerConfigured" />
	

   		<EventGenerator name="EG_ShipmentStarted"/>
		<EventSequence name="ES_Transport"/>
		<EventGenerator name="EG_ShipmentDelivered"/>
		
		
		
		
		
		<!-- Corellations for events -->

		<!-- alle Events im Durchlauf haben gleiche OrderId -->
		<EventCorrelation>
			<CorrelationData eventType="OrderReceived">
				<XPathSelector>
					<XPathExpression>//OrderId</XPathExpression>
				</XPathSelector>
			</CorrelationData>
			<CorrelationData eventType="OrderConfirmed">
				<XPathSelector>
					<XPathExpression>//OrderId</XPathExpression>
				</XPathSelector>
			</CorrelationData>
			<CorrelationData eventType="ShipmentTenderIssued">
				<XPathSelector>
					<XPathExpression>//OrderId</XPathExpression>
				</XPathSelector>
			</CorrelationData>
			<CorrelationData eventType="ShipmentTenderAccepted">
				<XPathSelector>
					<XPathExpression>//OrderId</XPathExpression>
				</XPathSelector>
			</CorrelationData>


			<CorrelationData eventType="DataLoggerConfigured">
				<XPathSelector>
					<XPathExpression>//OrderId</XPathExpression>
				</XPathSelector>
			</CorrelationData>
			<CorrelationData eventType="ShipmentStarted">
				<XPathSelector>
					<XPathExpression>//OrderId</XPathExpression>
				</XPathSelector>
			</CorrelationData>
			<CorrelationData eventType="TransportStart">
				<XPathSelector>
					<XPathExpression>//OrderId</XPathExpression>
				</XPathSelector>
			</CorrelationData>

			<CorrelationData eventType="TransportEnd">
				<XPathSelector>
					<XPathExpression>//OrderId</XPathExpression>
				</XPathSelector>
			</CorrelationData>
			<CorrelationData eventType="DataLoggerRead">
				<XPathSelector>
					<XPathExpression>//OrderId</XPathExpression>
				</XPathSelector>
			</CorrelationData>
			<CorrelationData eventType="ShipmentAudit">
				<XPathSelector>
					<XPathExpression>//OrderId</XPathExpression>
				</XPathSelector>
			</CorrelationData>

			<CorrelationData eventType="ShipmentDelivered">
				<XPathSelector>
					<XPathExpression>//OrderId</XPathExpression>
				</XPathSelector>
			</CorrelationData>

		</EventCorrelation>


		<!-- TransportStart CarrierId == ShipmentTenderAccepted CarrierId-->
		<EventCorrelation>
			<CorrelationData eventType="ShipmentTenderAccepted">
				<XPathSelector>
					<XPathExpression>//CarrierId</XPathExpression>
				</XPathSelector>
			</CorrelationData>
			<CorrelationData eventType="TransportStart">
				<XPathSelector>
					<XPathExpression>//CarrierId</XPathExpression>
				</XPathSelector>
			</CorrelationData>
		</EventCorrelation>
		
		<!-- Datalogger ID immer die Selbe -->
		<EventCorrelation>
			<CorrelationData eventType="DataLoggerConfigured">
				<XPathSelector>
					<XPathExpression>//LoggerId</XPathExpression>
				</XPathSelector>
			</CorrelationData>
			<CorrelationData eventType="DataLoggerRead">
				<XPathSelector>
					<XPathExpression>//LoggerId</XPathExpression>
				</XPathSelector>
			</CorrelationData>
		</EventCorrelation>
		
		<!-- alle ShipmentAudits gleiche Menge, Produkt wie OrderReceived-->
		<EventCorrelation>
			<CorrelationData eventType="OrderReceived">
				<XPathSelector>
					<XPathExpression>//ProductCollection</XPathExpression>
				</XPathSelector>
			</CorrelationData>
			<CorrelationData eventType="ShipmentAudit">
				<XPathSelector>
					<XPathExpression>//CheckedProducts</XPathExpression>
				</XPathSelector>
			</CorrelationData>
		</EventCorrelation>


		<!-- 
		  	Sequenz vom OrderReceive bis OrderConfirmed
			 -->
		<DateSequence>
			<CorrelationData eventType="OrderReceived">
				<XPathSelector>
					<XPathExpression>//DateTime</XPathExpression>
				</XPathSelector>
			</CorrelationData>
			<CorrelationData eventType="OrderConfirmed">
				<XPathSelector>
					<XPathExpression>//DateTime</XPathExpression>
				</XPathSelector>
			</CorrelationData>
			<RandomTimeSpans>
				<Min>
					<Minutes>1</Minutes>
				</Min>
				<Max>
					<Hours>12</Hours>
				</Max>
				<Granularity>Seconds</Granularity>
			</RandomTimeSpans>
		</DateSequence>

<!-- 
		  	Sequenz vom  OrderConfirmed bis ShipmentTenderIssued
			 -->
		<DateSequence>
			<CorrelationData eventType="OrderConfirmed">
				<XPathSelector>
					<XPathExpression>//DateTime</XPathExpression>
				</XPathSelector>
			</CorrelationData>
			<CorrelationData eventType="ShipmentTenderIssued">
				<XPathSelector>
					<XPathExpression>//DateTime</XPathExpression>
				</XPathSelector>
			</CorrelationData>
			<RandomTimeSpans>
				<Min>
					<Minutes>10</Minutes>
				</Min>
				<Max>
					<Hours>2</Hours>
				</Max>
				<Granularity>Seconds</Granularity>
			</RandomTimeSpans>
		</DateSequence>

	<!-- 
		  	Sequenz vom  ShipmentTenderIssued bis ShipmentAccepted
			 -->
		<DateSequence>
			<CorrelationData eventType="ShipmentTenderIssued">
				<XPathSelector>
					<XPathExpression>//DateTime</XPathExpression>
				</XPathSelector>
			</CorrelationData>
			<CorrelationData eventType="ShipmentTenderAccepted">
				<XPathSelector>
					<XPathExpression>//DateTime</XPathExpression>
				</XPathSelector>
			</CorrelationData>
			<RandomTimeSpans>
				<Min>
					<Minutes>20</Minutes>
				</Min>
				<Max>
					<Hours>24</Hours>
				</Max>
				<Granularity>Seconds</Granularity>
			</RandomTimeSpans>
		</DateSequence>


<!-- 
		  	Sequenz vom  OrderConfirmed bis DataLoggerConfigured
			 -->
		<DateSequence>
			<CorrelationData eventType="OrderConfirmed">
				<XPathSelector>
					<XPathExpression>//DateTime</XPathExpression>
				</XPathSelector>
			</CorrelationData>
			<CorrelationData eventType="DataLoggerConfigured">
				<XPathSelector>
					<XPathExpression>//DateTime</XPathExpression>
				</XPathSelector>
			</CorrelationData>
			<RandomTimeSpans>
				<Min>
					<Hours>5</Hours>
				</Min>
				<Max>
					<Hours>44</Hours>
				</Max>
				<Granularity>Seconds</Granularity>
			</RandomTimeSpans>
		</DateSequence>

 <!-- 
		  	Sequenz vom  OrderConfirmed bis ShipmentStarted
			Zwischen jeden Event  24Stunden -->
		<DateSequence>
			<CorrelationData eventType="OrderConfirmed">
				<XPathSelector>
					<XPathExpression>//DateTime</XPathExpression>
				</XPathSelector>
			</CorrelationData>
			<CorrelationData eventType="ShipmentStarted">
				<XPathSelector>
					<XPathExpression>//DateTime</XPathExpression>
				</XPathSelector>
			</CorrelationData>
			<RandomTimeSpans>
				<Min>
					<Hours>48</Hours>
				</Min>
				<Max>
					<Hours>72</Hours>
				</Max>
				<Granularity>Seconds</Granularity>
			</RandomTimeSpans>
		</DateSequence>

  <!-- 
		  	Sequenz vom  ShipmentStarted bis erstes TransportStart
			Zwischen jeden Event  24Stunden -->

		<DateSequence>
			<CorrelationData eventType="ShipmentStarted">
				<XPathSelector>
					<XPathExpression>//DateTime</XPathExpression>
				</XPathSelector>
			</CorrelationData>
			<CorrelationData eventType="TransportStart">
				<XPathSelector>
					<XPathExpression>//DateTime</XPathExpression>
				</XPathSelector>
			</CorrelationData>
			<RandomTimeSpans>
				<Min>
					<Seconds>1</Seconds>
				</Min>
				<Max>
					<Seconds>60</Seconds>
				</Max>
				<Granularity>Seconds</Granularity>
			</RandomTimeSpans>
		</DateSequence>
			

			
		
		
	</EventSequence>
	
	
	
	
	
	
	
	
	

	<!--
    ***************************************
    **** Simulator Run Definitions ****
    ***************************************
    -->
	<SimulationRun name="ES_RouteVienna_LaPlata">
		<EventSequenceSimulation>
			<EventSequence name="ES_Route"/>
			<Instances>1</Instances>
			<Concurrency>Serialized</Concurrency>
		</EventSequenceSimulation>
	</SimulationRun>



