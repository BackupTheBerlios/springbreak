<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>

<class 
    name="at.generic.eventmodel.OrderConfirmedEvent" 
    table="orderconfirmed"
>

    <id
        name="id"
        type="long"
        column="id"
    >
        <generator class="assigned" />
    </id>

    <property
        name="originalguid"
        type="java.lang.String"
        column="originalguid"
        length="255"
    />
    <property
        name="priority"
        type="java.lang.String"
        column="priority"
        length="255"
    />
    <property
        name="severity"
        type="java.lang.String"
        column="severity"
        length="255"
    />
    <property
        name="localtimecreated"
        type="java.sql.Date"
        column="localtimecreated"
        length="4"
    />
    <property
        name="localtimecreatedrw"
        type="java.sql.Date"
        column="localtimecreatedrw"
        length="4"
    />
    <property
        name="utctimecreated"
        type="java.sql.Date"
        column="utctimecreated"
        length="4"
    />
    <property
        name="utctimecreatedrw"
        type="java.sql.Date"
        column="utctimecreatedrw"
        length="4"
    />
    <property
        name="majorversion"
        type="java.lang.String"
        column="majorversion"
        length="255"
    />
    <property
        name="minorversion"
        type="java.lang.String"
        column="minorversion"
        length="255"
    />
    <property
        name="orderid"
        type="java.lang.String"
        column="orderid"
        length="255"
    />
    <property
        name="datetime"
        type="java.lang.String"
        column="datetime"
        length="255"
    />
    
    <!-- Associations -->
  
    <!-- bi-directional one-to-many association to Productcollection -->

</class>
</hibernate-mapping>
