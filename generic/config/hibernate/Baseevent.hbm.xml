<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<class 
    name="at.generic.eventmodel.BaseEvent" 
    table="baseevent"
>

    <id
        name="id"
        type="java.lang.Integer"
        column="id"
    >
        <generator class="assigned" />
    </id>

    <property
        name="guid"
        type="java.lang.String"
        column="guid"
        length="36"
    />
    <property
        name="eventXml"
        type="java.lang.String"
        column="eventxml"
        length="7800"
    />
    <property
        name="dbtimeCreated"
        type="java.sql.Timestamp"
        column="dbtimeCreated"
        length="23"
    />
    <property
        name="eventtype"
        type="java.lang.String"
        column="eventtype"
        length="255"
    />

    <!-- Associations -->
  
    <!-- bi-directional one-to-many association to Correlationset -->
 
</class>
</hibernate-mapping>
